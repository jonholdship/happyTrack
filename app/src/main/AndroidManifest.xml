<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.ducksaxaphone.happytrack">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".ActivityTrack"
            android:theme="@style/AppTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ActivityCalendar"
            android:label="@string/title_activity_calendar"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".ActivityNotify"
            android:label="@string/title_activity_notify"
            android:theme="@style/AppTheme" />

        <receiver
            android:name=".AlarmReceiver"
            android:process=":remote" />

        <activity
            android:name=".ActivityAbout"
            android:label="ActivityAbout"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".ActivityAnalysis"
            android:label="@string/title_activity_analysis"
            android:theme="@style/AppTheme">


        </activity>
    </application>

</manifest>